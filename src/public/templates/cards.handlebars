<div class="row row-cols-1 row-cols-md-3 g-4">
   {{#each products}}
  <div class="col">
    <div class="card">
      <img src={{this.thumbnail}} class="card-img-top img-fluid img-thumbnail" alt="producto"/>
      <div class="card-body">
        <h5 class="card-title">{{this.title}}</h5>
        <p class="card-text">Precio: ${{this.price}}</p>
        <a href="/products/{{this.id}}" class="btn btn-primary">Detalles</a>
        <a href="#" class="btn btn-primary">AÃ±adir al Carrito</a>
      </div>
    </div>
  </div>





    {{/each}}
</div>
